<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>taxcalculationdemo</display-name>
  <welcome-file-list>
    <welcome-file>calculate.html</welcome-file>
   
  </welcome-file-list>
  <servlet>
      <servlet-name>calculate</servlet-name>
       <servlet-class>com.training.servlet.CalculateServlet</servlet-class>
       <init-param>
           <param-name>slb1</param-name>
           <param-value>250000</param-value>       
       
       </init-param>
       <init-param>
           <param-name>slb2</param-name>
           <param-value>500000</param-value>       
       
       </init-param>
       <init-param>
           <param-name>interest</param-name>
           <param-value>0.85</param-value>       
       
       </init-param>
   </servlet>
   <servlet-mapping>
      <servlet-name>calculate</servlet-name>
       <url-pattern>/calculate</url-pattern>
   </servlet-mapping>
  
</web-app>